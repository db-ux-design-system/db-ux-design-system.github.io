{
  "extends": "astro/tsconfigs/strict",
  "include": [
    "./content",
    "./template",
    "./app.config.ts",
    "./app.navigation.ts",
    "./components"
  ],
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@root/*": ["./*"],
      "@content/*": ["content/*"],
      "@template/*": ["template/*"],
      "@components/*": ["components/*"],
      "@config": ["app.config.ts"]
    }
  }
}
